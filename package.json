{
  "name": "start-template-standard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run generate:dev -- --watch\" \"next dev --turbopack\"",
    "build": "npm run generate:build && next build",
    "generate:dev": "basehub --draft",
    "generate:build": "basehub",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "basehub": "^8.2.0",
    "next": "15.3.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.1.2",
    "eslint": "^9",
    "eslint-config-next": "15.3.1",
    "prettier": "^3.5.3",
    "tailwindcss": "^4",
    "typescript": "^5"
  },
  "prettier": {
    "semi": false
  }
}
